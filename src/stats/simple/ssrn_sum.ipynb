   "source": [
    "# ssrn_sum\n",
    "Simple statistics with random number to calculate sum"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "0b2df804",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Numbers are [10, 16, 20, 16, 20]\n",
      "The sum is 82\n"
     ]
    }
   ],
   "source": [
    "import random\n",
    "\n",
    "N = 5\n",
    "nums = []\n",
    "xmin = 5\n",
    "xmax = 20\n",
    "\n",
    "for i in range(0, N):\n",
    "    x = random.randint(xmin, xmax)\n",
    "    nums.append(x)\n",
    "\n",
    "print(\"Numbers are\", nums)\n",
    "print(\"The sum is\", sum(nums))"
   ]
